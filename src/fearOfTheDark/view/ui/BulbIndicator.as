package fearOfTheDark.view.ui{	import flash.display.*;		public class BulbIndicator extends MovieClip	{		private var numBulbs:int;				public function BulbIndicator():void { }				public function setBulbsNumber(b:int):void		{			numBulbs = b;						bulb5.visible = (numBulbs >= 5);			bulb4.visible = (numBulbs >= 4);			bulb3.visible = (numBulbs >= 3);			bulb2.visible = (numBulbs >= 2);			bulb1.visible = (numBulbs >= 1);		}	}}